{
  "name": "@emartech/google-big-query",
  "description": "Helper library to easily use Google BigQuery in NodeJS",
  "engines": {
    "node": "8.9.1"
  },
  "main": "src/index.js",
  "scripts": {
    "test": "mocha test-setup.spec.js src --recursive",
    "code-style": "eslint src/",
    "validate-commit-msg": "validate-commit-msg",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "release": {
    "verifyConditions": {
      "path": "./node_modules/semantic-release/src/lib/plugin-noop.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emartech/google-big-query-js.git"
  },
  "author": "Emarsys",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emartech/google-big-query-js/issues"
  },
  "pre-commit": [
    "code-style",
    "validate-commit-msg"
  ],
  "homepage": "https://github.com/emartech/google-big-query-js",
  "peerDependencies": {
    "config": "1.28.1",
    "google-cloud": "0.57.0"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "chai-subset": "1.6.0",
    "co-mocha": "1.2.1",
    "eslint": "4.11.0",
    "eslint-config-emarsys": "4.0.0",
    "eslint-plugin-no-only-tests": "2.0.0",
    "eslint-plugin-require-path-exists": "1.1.7",
    "eslint-plugin-security": "1.4.0",
    "from2": "2.3.0",
    "from2-array": "0.0.4",
    "from2-string": "1.1.0",
    "mocha": "4.0.1",
    "pre-commit": "1.2.2",
    "sinon": "4.1.2",
    "sinon-chai": "2.14.0",
    "stream-to-array": "2.3.0",
    "validate-commit-message": "3.0.1",
    "semantic-release": "8.2.0"
  },
  "dependencies": {
    "@emartech/google-cloud-storage": "1.1.2",
    "config": "1.28.1",
    "google-cloud": "0.57.0",
    "pumpify": "1.3.5",
    "split": "1.0.1"
  },
  "version": "1.4.1"
}
